{
  "name": "simple-customize-markdown-converter",
  "version": "1.3.2",
  "type": "module",
  "description": "Transform Markdown into fully customizable HTML or React components.",
  "keywords": [
    "markdown",
    "html",
    "converter",
    "react-markdown",
    "typescript"
  ],
  "author": "Regiko04",
  "license": "MIT",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.js",
      "require": "./dist/react.cjs"
    }
  },
  "scripts": {
    "test": "jest",
    "build": "tsup src/index.ts src/react.tsx --format cjs,esm --dts --clean --minify --splitting",
    "start": "ts-node src/index.ts"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.2.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.2.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.2.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Riiichan04/simple-custom-markdown-converter.git"
  },
  "bugs": {
    "url": "https://github.com/Riiichan04/simple-custom-markdown-converter/issues"
  },
  "homepage": "https://github.com/Riiichan04/simple-custom-markdown-converter#readme"
}